{
    "name": "pickles2/px2-git",
    "description": "Pickles 2 のプロジェクトデータに最適化された git 関連APIを提供します。",
    "keywords": ["pickles2"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tomoya Koyanagi",
            "email": "tomk79@gmail.com"
        }
    ],
    "scripts":{
        "test": [
            "php ./vendor/phpunit/phpunit/phpunit"
        ] ,
        "documentation": [
            "php ./vendor/phpdocumentor/phpdocumentor/bin/phpdoc --title \"px2-git for Pickles 2 API Document\" -d \"./php/\" -t \"./docs/phpdoc/\""
        ]
    } ,
    "repositories": [
    ],
    "require": {
        "php": ">=5.4.0" ,
        "tomk79/filesystem": "^1.0",
        "tomk79/request": "^1.0",
        "tomk79/phpgit": "0.3.*"
    },
    "require-dev": {
        "broccoli-html-editor/broccoli-module-fess": "*",
        "broccoli-html-editor/broccoli-module-plain-html-elements": "*",
        "pickles2/px-fw-2.x": "dev-develop",
        "pickles2/px2-dec": "dev-master",
        "pickles2/px2-multitheme": "dev-develop",
        "pickles2/px2-path-resolver": "dev-develop",
        "pickles2/px2-sitemapexcel": "dev-develop",
        "phpunit/phpunit": "4.1.*" ,
        "phpdocumentor/phpdocumentor": "2.8.*"
    },
    "autoload": {
        "files": [
            "php/px2-git.php"
        ]
    }
}
